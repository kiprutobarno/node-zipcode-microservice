{
  "name": "node-zip-microservice",
  "version": "1.0.0",
  "description": "The business requirement for this service is to accept two Zip Codes and return the distance between them in miles.",
  "main": ".src/server.js",
  "repository": "https://github.com/ywalakamar/node-zip-microservice.git",
  "author": "Kipruto Barno",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "eslint ./src/",
    "dev": "nodemon --exec babel-node src/server.js",
